<h1>Venda #<%= v.id %></h1>
<p class="muted"><%= v.created_at %></p>
<table class="table">
  <thead><tr><th>Produto</th><th>Qtd</th><th>Preço</th><th>Subtotal</th></tr></thead>
  <tbody>
  <% let total=0; itens.forEach(it => { const st = it.qty*it.price_cents; total+=st; %>
    <tr><td><%= it.name %></td><td><%= it.qty %></td><td>€ <%= (it.price_cents/100).toFixed(2) %></td><td>€ <%= (st/100).toFixed(2) %></td></tr>
  <% }); %>
  </tbody>
  <tfoot><tr><th colspan="3" style="text-align:right">Total</th><th>€ <%= (total/100).toFixed(2) %></th></tr></tfoot>
</table>